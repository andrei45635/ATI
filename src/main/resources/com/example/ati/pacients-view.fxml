<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ati.controller.PacientsViewController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="366.0">
         <center>
            <TableView fx:id="pacientTableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="pacientCNPColumn" prefWidth="113.0" text="pacientCNP" />
                <TableColumn fx:id="pacientSeverityColumn" prefWidth="107.0" text="pacientSeverity" />
                  <TableColumn fx:id="pacientDiagnosticColumn" prefWidth="145.0" text="pacientDiagnostic" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
      <Button fx:id="addPacientButton" layoutX="426.0" layoutY="75.0" mnemonicParsing="false" onAction="#onClickAddPacient" prefHeight="30.0" prefWidth="105.0" text="Add pacient">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <ComboBox fx:id="bedTypeComboBox" layoutX="404.0" layoutY="188.0" prefWidth="150.0" promptText="BedType" />
   </children>
</AnchorPane>
